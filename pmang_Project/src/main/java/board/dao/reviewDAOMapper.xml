<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="reviewSQL">
 	<insert id="reviewWrite" parameterType="review">
 		insert into review values(review_seq.nextval,#{buyerNum},#{itemNum},#{reviewWriter},#{reviewSubject},#{reviewContent},#{reviewImg1},#{reviewImg2},#{reviewImg3},sysdate,#{reviesStamp})
 		
 	</insert>
 </mapper>